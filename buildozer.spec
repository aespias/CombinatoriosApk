[app]
title = AngelAPK
package.name = angelapk
package.domain = org.example
source.dir = .
version = 0.1
requirements = python3,kivy,numpy,pandas,tensorflow  # Agrega otras bibliotecas necesarias
orientation = portrait
# (int) Android API version to use
android.api = 30  # Usa 30 o 29 según sea necesario
# (int) Android SDK version to use
android.minapi = 21  # Puedes usar 21 o un valor mayor según tu necesidad
# (int) Android NDK version to use
android.ndk = 21b  # Version de NDK que deseas usar

